{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "indent": ["error", 4],
        "no-console": "off",
        "no-underscore-dangle": 0,
        "object-curly-newline": 0,
        "camelcase": ["error", { "properties": "never", "ignoreDestructuring": true }],
        "max-len": ["error", { "code": 150}]
    }
}
